<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="piano.controller.MainController">
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
            <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#onCancel" text="cancel" />
            <Button fx:id="buttonSave" mnemonicParsing="false" onAction="#onSave" text="save" />
        </buttons>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </ButtonBar>
   </bottom>
   <top>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button fx:id="buttonDelete" mnemonicParsing="false" onAction="#onDelete" text="delete" />
            <Button fx:id="buttonNew" mnemonicParsing="false" onAction="#onNew" text="new" />
        </buttons>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </ButtonBar>
   </top>
   <left>
      <ListView fx:id="listViewTones" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </left>
   <center>
      <FlowPane fx:id="flowPaneTone" BorderPane.alignment="CENTER">
         <children>
            <GridPane hgap="5.0" prefHeight="90.0" prefWidth="360.0">
              <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="frequency" GridPane.rowIndex="2" />
                  <Label text="note" GridPane.rowIndex="1" />
                  <Label text="velocity" GridPane.rowIndex="3" />
                  <Slider fx:id="sliderVelocity" max="127.0" minorTickCount="1" showTickLabels="true" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <ComboBox fx:id="comboBoxNote" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="valueFrequency" text="-" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="object" />
                  <Label fx:id="valueObject" text="-" GridPane.columnIndex="1" />
               </children>
            </GridPane>
         </children>
      </FlowPane>
   </center>
</BorderPane>
